								}
								
							} else if(getType(exprSegundo).equals("LLAMADOFUNCION")) {
								
								if((checkFunctionForOperatorType(exprSegundo, "DALAS"))) {
									
									error("Tu operacion no es completamente DALAS, infeliz", PolePackage.Literals.EXPRESION_CONCATENACION__SEGUNDO);
									
								}
								
