						}
		
						
						
					} else if (getType(exprDer).equals("LLAMADOVARIABLE")) {
						
						if(checkVariableForOperatorType(exprDer,"MRBEAST") && checkVariableForOperatorType(exprDer,"XOKAS")) {
						
							
							error("Tu operacion no es completamente MRBEAST O XOKAS, infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
						}
						
						if(!(getType(exprIzq).equals("MRBEAST"))) {
							
							if(getType(exprIzq).equals("LLAMADOFUNCION")) {
								
								if(checkFunctionForOperatorType(exprIzq, "MRBEAST") && checkFunctionForOperatorType(exprIzq, "XOKAS")) {
									
