	@Check
	public void checkNumberOfArguments(LlamadoFuncion llamado) {
		
		if(llamado.getArgs().size() != llamado.getFunc().getParam().size()) {
			
			error("Pon bien los argumentos, infeliz", PolePackage.Literals.LLAMADO_FUNCION__ARGS);
			
		}
		
		
	}
	
	@Check
	
	public void checkNumberOfArgumentsMethod(LlamadoMetodo llamado) { //Explicar Joseph
		
		if(llamado.getArgs().size() != llamado.getMethod().getParam().size()) {
			
			error("No pusiste bien tu numero de argumentos, infeliz", PolePackage.Literals.LLAMADO_METODO__ARGS);
			
		}
		
	}
	
	
	
	@Check
	public void checkExpressionTypeInDeclaration(DeclVariables decl) {
		
		checkAssignationAndDeclarationType(decl);
		
	}
	
	@Check
	
	public void checkTypeInAssignation(AsignacionVariable asig) {
		
		checkAssignationAndDeclarationType(asig);
		
	}
	
	
	@Check
	public void checkExpressionsInOperators(Expresion expr) {
		
		if(expr instanceof ExpresionAritmetica) { 
			
			Expresion exprIzq = ((ExpresionAritmetica) expr).getOp().getIzq();
			Expresion exprDer = ((ExpresionAritmetica) expr).getOp().getDer();
			
			if(!(getType(exprIzq).equals("MRBEAST") && getType(exprDer).equals("MRBEAST"))) {
				
				if(getType(exprIzq).equals("LLAMADOVARIABLE") || getType(exprDer).equals("LLAMADOVARIABLE")) {
					
					if(getType(exprIzq).equals("LLAMADOVARIABLE")) {
						
