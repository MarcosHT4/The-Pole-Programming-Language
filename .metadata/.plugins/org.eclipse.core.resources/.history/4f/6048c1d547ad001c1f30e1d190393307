					if((checkVariableForOperatorType(exprIzq,"MRBEAST") && checkVariableForOperatorType(exprIzq,"XOKAS"))) {
						
						error("Tu operacion no es completamente MRBEAST O XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
						
					}
					
					if(!(getType(exprDer).equals("MRBEAST"))) {
						
						if(getType(exprDer).equals("LLAMADOVARIABLE")) {
							
							if((checkVariableForOperatorType(exprDer, "MRBEAST") && checkVariableForOperatorType(exprDer, "XOKAS"))) {
								
								error("Tu operacion no es completamente MRBEAST O XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
								
								
							}
							
						} else if(getType(exprDer).equals("LLAMADOFUNCION")) {
							
							if((checkFunctionForOperatorType(exprDer, "MRBEAST") && checkFunctionForOperatorType(exprDer, "XOKAS"))) {
								
								error("Tu operacion no es completamente MRBEAST o XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
								
							}
							
						} else {
							
							error("Pon bien el MRBEAST o el XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
						}
						
					}
	
					
					
				} else if (getType(exprDer).equals("LLAMADOVARIABLE")) {
					
					if(checkVariableForOperatorType(exprDer,"MRBEAST") && checkVariableForOperatorType(exprDer,"XOKAS")) {
					
						
						error("Tu operacion no es completamente MRBEAST O XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
					}
					
					if(!(getType(exprIzq).equals("MRBEAST"))) {
						
						if(getType(exprIzq).equals("LLAMADOFUNCION")) {
							
							if(checkFunctionForOperatorType(exprIzq, "MRBEAST") && checkFunctionForOperatorType(exprIzq, "XOKAS")) {
								
								error("Tu operacion no es completamente MRBEAST o XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
								
							}
							
							
						} else {
							
							error("Pon bien el MRBEAST o el XOKAS infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
							
						}
						
					}
					
				}
			
			
				
			} else if(getType(exprIzq).equals("LLAMADOFUNCION") || getType(exprDer).equals("LLAMADOFUNCION")) {
				
				if(getType(exprIzq).equals("LLAMADOFUNCION")) {
					
					if(checkFunctionForOperatorType(exprIzq, "MRBEAST") && checkFunctionForOperatorType(exprIzq, "XOKAS")) {
						
						error("Tu funcion de la izquierda no es MRBEAST o XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
						
					}
					
					if(!(getType(exprDer).equals("MRBEAST"))) {
						
						if(getType(exprDer).equals("LLAMADOVARIABLE")) {
							
							if((checkVariableForOperatorType(exprDer, "MRBEAST") && checkVariableForOperatorType(exprDer, "XOKAS"))) {
								
								error("Tu operacion no es completamente MRBEAST O XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
								
								
							}
							
						} else if(getType(exprDer).equals("LLAMADOFUNCION")) {
							
							if((checkFunctionForOperatorType(exprDer, "MRBEAST") && checkFunctionForOperatorType(exprDer, "XOKAS"))) {
								
								error("Tu operacion no es completamente MRBEAST o XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
								
							}
							
						} else {
							
							error("Pon bien el Dalas, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
						}
						
					}
					
					
					
					
			} else if (getType(exprDer).equals("LLAMADOFUNCION")) {
				
					if(checkFunctionForOperatorType(exprDer, "MRBEAST") && checkFunctionForOperatorType(exprDer, "XOKAS")) {
						
						error("Tu funcion de la derecha no es MRBEAST o XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
						
					}

			}
			
		} else {
			
			error("No pusiste bien el MRBEAST o XOKAS, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION_ARITMETICA__OP);
			
		}
	}
		
			
	} else if(expr instanceof ExpresionLongitudString) {
		
		Expresion llamado =  ((ExpresionLongitudString) expr).getLlamado();
		
		if(getType(llamado).equals("LLAMADOVARIABLE")) {
			

			if(checkVariableForOperatorType(llamado, "DALAS")) {
				
				error("No se puede sacar el largo de una variable que no sea DALAS, infeliz", PolePackage.Literals.EXPRESION_LONGITUD_STRING__LLAMADO);
				
			}
			
		} else {
			
			error("Solo se permiten llamado de variables de tipo DALAS, infeliz", PolePackage.Literals.EXPRESION_LONGITUD_STRING__LLAMADO);
			
		}
		
		
		
		
		
	}
}
	
	@Check

	public void checkTypeOfArgumentsAndParameters(LlamadoFuncion llamado) {
		
		checkMethodAndFunctionParametersWithCall(null, llamado, "FUNCTION");		
	}
	
	@Check
	
	public void checkTypeOfArgumentsAndParametersInMethod(LlamadoMetodo llamado) { 
		
		checkMethodAndFunctionParametersWithCall(llamado, null, "METHOD");
		
	}
	
	@Check
	
	public void checkCallMethodType(LlamadoMetodo llamado) { 
		
		if(!llamado.getMethod().getTipoDeDato().equals("MICORAZON")) {
			
			error("Un llamado de tipo " + llamado.getMethod().getTipoDeDato() + " no es una instrucción, infeliz", PolePackage.Literals.LLAMADO_METODO__METHOD);
			
		}
		
	}
	
	@Check
	
	public void checkSwitchCases(Switch sw) { 
		
		if(sw.getVar().getVariable().getTipoDeDato().equals("DALAS")) {
			
			error("No se permiten variables de tipo DALAS, infeliz", PolePackage.Literals.SWITCH__VAR);
			
		}
		
		for(CaseSwitch cas: sw.getCase()) {
			
			Expresion expr = cas.getE();
			
			if(!(sw.getVar().getVariable().getTipoDeDato().equals(getType(expr)))) {
				
				if(!(sw.getVar().getVariable().getTipoDeDato().equals("XOKAS") && getType(expr).equals("MRBEAST")))
				
				error("Tus cases no coinciden con tu variable, imbecil", PolePackage.Literals.SWITCH__CASE);
				
			}
			
			if(getOperationType(expr).equals("OPAR")) {
				
				ExpresionAritmetica exprA = ((ExpresionAritmetica) expr);
				if(getType(exprA.getOp().getIzq()).equals("LLAMADOVARIABLE") ||
					getType(exprA.getOp().getIzq()).equals("LLAMADOFUNCION") ||
					getType(exprA.getOp().getDer()).equals("LLAMADOVARIABLE") ||
					getType(exprA.getOp().getDer()).equals("LLAMADOFUNCION")) {
					
					error("No se permiten llamados de variables o funciones, infeliz", PolePackage.Literals.SWITCH__CASE);
					
					
				}
				
			
				
			} else if(getOperationType(expr).equals("OPBC")) {
				
				OperacionesBooleanasDeComparacion exprBC = ((OperacionesBooleanasDeComparacion) expr);
				if(getType(exprBC.getIzq()).equals("LLAMADOVARIABLE") ||
						getType(exprBC.getIzq()).equals("LLAMADOFUNCION") ||
						getType(exprBC.getDer()).equals("LLAMADOVARIABLE") ||
						getType(exprBC.getDer()).equals("LLAMADOFUNCION")) {
						
						error("No se permiten llamados de variables o funciones, infeliz", PolePackage.Literals.SWITCH__CASE);
						
						
					}
				
			} else if(getOperationType(expr).equals("OPBCA")) {
				
				OperacionesBooleanasDeComparacionAritmetica exprBCA = ((OperacionesBooleanasDeComparacionAritmetica) expr);
				if(getType(exprBCA.getOp().getIzq()).equals("LLAMADOVARIABLE") ||
						getType(exprBCA.getOp().getIzq()).equals("LLAMADOFUNCION") ||
						getType(exprBCA.getOp().getDer()).equals("LLAMADOVARIABLE") ||
						getType(exprBCA.getOp().getDer()).equals("LLAMADOFUNCION")) {
						
						error("No se permiten llamados de variables o funciones, infeliz", PolePackage.Literals.SWITCH__CASE);
						
						
					}
				
			} else if (getOperationType(expr).equals("OLB")) {
				
				OperadoresLogicosBooleanos exprLB = ((OperadoresLogicosBooleanos) expr);
				if(getType(exprLB.getIzq()).equals("LLAMADOVARIABLE") ||
						getType(exprLB.getIzq()).equals("LLAMADOFUNCION") ||
						getType(exprLB.getDer()).equals("LLAMADOVARIABLE") ||
						getType(exprLB.getDer()).equals("LLAMADOFUNCION")) {
						
						error("No se permiten llamados de variables o funciones, infeliz", PolePackage.Literals.SWITCH__CASE);
						
						
					}
				
			} else if(getOperationType(expr).equals("OPN")) {
				
				OpNEG exprN = ((OpNEG) expr);
				if(getType(exprN.getExpr()).equals("LLAMADOVARIABLE")||
					getType(exprN.getExpr()).equals("LLAMADOFUNCION")) {
					
					error("No se permiten llamdos de variables o funciones, infeliz", PolePackage.Literals.SWITCH__CASE);
					
					
				}
				
			}
 			
			
			
		
		}
		
	}
	
	@Check
	
	public void checkReturnType(Funciones func) {
		
		if(func.getTipoDeDato().equals("MICORAZON") && func.getE()!=null) {
			
			error("Una funcion de tipo MICORAZON, no devuelve nada, tarado", PolePackage.Literals.FUNCIONES__TIPO_DE_DATO);
			
		}
		
		if(!(func.getTipoDeDato().equals(getType(func.getE()))) && !(func.getTipoDeDato().equals("MICORAZON"))) {
			
			if(getType(func.getE()).equals("LLAMADOVARIABLE")) {
				
				if(checkVariableForOperatorType(func.getE(), func.getTipoDeDato())) {
					
					if(!(func.getTipoDeDato().equals("XOKAS") &&  getVariableType(func.getE()).equals("MRBEAST"))) {
					
					error("Tu variable en tu tipo de retorno, no coincide con el tipo de la funcion tonto a las 3 en punto", PolePackage.Literals.FUNCIONES__E);
					
				}
					
				}
				
			} else if (getType(func.getE()).equals("LLAMADOFUNCION")) {
				
				if(checkFunctionForOperatorType(func.getE(), func.getTipoDeDato())) {
					
					if(!(func.getTipoDeDato().equals("XOKAS") && getFunctionType(func.getE()).equals("MRBEAST"))) {
				
					
					error("Tu funcion en tu tipo de retorno, no coincide con el tipo de la funcion, tonto a las 3 en punto", PolePackage.Literals.FUNCIONES__E);
					
					}
					
				}
				
			} else if(!(func.getTipoDeDato().equals("XOKAS") && getType(func.getE()).equals("MRBEAST"))) {
				
				error("Tu tipo de retorno, no coincide, tonto a las 3 en punto", PolePackage.Literals.FUNCIONES__E);
				
			}
			
		}
		
	}
	
	@Check
	
	public void checkBooleanOperators(ExpresionOperacionesBooleanas expr) {
		
		if(getOperationType(expr).equals("OLB")) {
			
			OperadoresLogicosBooleanos op = ((OperadoresLogicosBooleanos)expr);
			if(op.getX().equals("PEPE") && !op.getY().equals("PANDA")) {
				
				error("Esta expresion booleana no existe, tonto a las 9 en punto", PolePackage.Literals.OPERADORES_LOGICOS_BOOLEANOS__X);
				
			} else if (op.getX().equals("NAR") && !op.getY().equals("ANJA")) {
				
				error("Esta expresion booleana no existe, tonto a las 9 en punto", PolePackage.Literals.OPERADORES_LOGICOS_BOOLEANOS__X);
				
			} else if (op.getX().equals("XO") && !op.getY().equals("R")) {
				
				error("Esta expresion booleana no existe, tonto a las 9 en punto", PolePackage.Literals.OPERADORES_LOGICOS_BOOLEANOS__X);
				
			}
			
		} else if(getOperationType(expr).equals("OPBC")) {
			
			OperacionesBooleanasDeComparacion op = ((OperacionesBooleanasDeComparacion) expr);
			if(op.getX().equals("POXI") && !op.getY().equals("POL")) {
				
				error("Esta expresion booleana no existe, tonto a las 9 en punto", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__X);
				
			} else if(op.getX().equals("AGUA") && !op.getY().equals("RRAS")) {
				
				error("Esta expresion booleana no existe, tonto a las 9 en punto", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__X);
				
			}
			
		}
		
	}
	
	
	
	private void checkAssignationAndDeclarationType(Instruccion instr) {
		
		if(instr instanceof DeclVariables) {
			
			DeclVariables decl = ((DeclVariables)instr);
			
			if(decl.getTipoDeDato().equals("DALAS")) {
				
				if(!(decl.getValor() instanceof ExpresionString)) {
					
					if(decl.getValor() instanceof LlamadoVariable) { //PEREMENNAYA DALAS blalbla -> variable
						
						if(checkVariableType(decl, "DALAS")) {
							
							error("Tu variable no es de tipo DALAS, infeliz" ,PolePackage.Literals.DECL_VARIABLES__VALOR);
							
						}
						
						
					} else if(decl.getValor() instanceof LlamadoFuncion) {
						
						if(checkFunctionType(decl, "DALAS")) {
							
							error("Tu funcion no devuelve DALAS, infeliz", PolePackage.Literals.DECL_VARIABLES__VALOR);
							
							
						}
						
					} else if(decl.getValor()!=null) {
						
						error("Tu expresion no es de tipo DALAS, tarado", PolePackage.Literals.DECL_VARIABLES__VALOR);
						
					}
			
				} 
					
				
			} else if (decl.getTipoDeDato().equals("MRBEAST") || decl.getTipoDeDato().equals("XOKAS")) {
				
				if(!(decl.getValor() instanceof ExpresionDecimal)) {
					
					if(decl.getValor() instanceof LlamadoVariable) {
						
						if(checkVariableType(decl, "MRBEAST") && checkVariableType(decl, "XOKAS")) {
							
							error("Tu variable no es de tipo MRBEAST o XOKAS, infeliz", PolePackage.Literals.DECL_VARIABLES__VALOR);
							
						}
						
					} else if(decl.getValor() instanceof LlamadoFuncion) {
						
						if(checkFunctionType(decl, "MRBEAST") && checkFunctionType(decl, "XOKAS")) {
							
							error("Tu funcion no es de tipo MRBEAST o XOKAS, infeliz",PolePackage.Literals.DECL_VARIABLES__VALOR);
							
						}
						
					} else if (decl.getValor() != null) {
						
						error("Tu expresion no es de tipo MRBEAST o XOKAS, infeliz", PolePackage.Literals.DECL_VARIABLES__VALOR);
						
					}
					
					
				}
				
			} else if (decl.getTipoDeDato().equals("YINYANG")) {
				
				if(!(decl.getValor() instanceof ExpresionBooleana)) {
					
					if(decl.getValor() instanceof LlamadoVariable) {
						
						if(checkVariableType(decl, "YINYANG")) {
							
							error("Tu variable no es de tipo YINYANG, infeliz", PolePackage.Literals.DECL_VARIABLES__VALOR);
							
						}
						
					} else if(decl.getValor() instanceof LlamadoFuncion) {
						
						if(checkFunctionType(decl, "YINYANG")) {
							
							error("Tu funcion no es de tipo YINYANG, infeliz",PolePackage.Literals.DECL_VARIABLES__VALOR);
							
						}
						
					} else if(decl.getValor()!=null) {
						
						error("Tu expresion no es de tipo YINYANG, infeliz", PolePackage.Literals.DECL_VARIABLES__VALOR);
						
					}
					
				}
				
			} else if(decl.getTipoDeDato().equals("MICORAZON")) {
				
				error("Una variable, no puede ser de tipo MICORAZON, tarado", PolePackage.Literals.DECL_VARIABLES__TIPO_DE_DATO);
				
			}
			
			
		} else if (instr instanceof AsignacionVariable) {
			
			AsignacionVariable asig = ((AsignacionVariable) instr);
			
			if(asig.getVariable().getTipoDeDato().equals("DALAS")) {
				
				if(!(asig.getValor() instanceof ExpresionString)) {
					
					if(asig.getValor() instanceof LlamadoVariable) { //PEREMENNAYA DALAS blalbla -> variable
						
						if(checkVariableType(asig, "DALAS")) {
							
							error("Tu variable no es de tipo DALAS, infeliz" ,PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
							
						}
						
						
					} else if(asig.getValor() instanceof LlamadoFuncion) {
						
						if(checkFunctionType(asig, "DALAS")) {
							
							error("Tu funcion no devuelve DALAS, infeliz", PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
							
							
						}
						
					} else {
						
						error("Pon bien el DALAS, infeliz", PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
						
					}
			
				} 
					
				
			} else if (asig.getVariable().getTipoDeDato().equals("MRBEAST") || asig.getVariable().getTipoDeDato().equals("XOKAS")) {
				
				if(!(asig.getValor() instanceof ExpresionDecimal)) {
					
					if(asig.getValor() instanceof LlamadoVariable) {
						
						if(checkVariableType(asig, "MRBEAST") && checkVariableType(asig, "XOKAS")) {
							
							error("Tu variable no es de tipo MRBEAST o XOKAS, infeliz", PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
							
						}
						
					} else if(asig.getValor() instanceof LlamadoFuncion) {
						
						if(checkFunctionType(asig, "MRBEAST") && checkFunctionType(asig, "XOKAS")) {
							
							error("Tu funcion no es de tipo MRBEAST, infeliz",PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
							
						}
						
					} else {
						
						error("Pon bien el MRBEAST, infeliz o XOKAS", PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
						
					}
					
					
				}
				
			} else if (asig.getVariable().getTipoDeDato().equals("YINYANG")) {
				
				if(!(asig.getValor() instanceof ExpresionBooleana)) {
					
					if(asig.getValor() instanceof LlamadoVariable) {
						
						if(checkVariableType(asig, "YINYANG")) {
							
							error("Tu variable no es de tipo YINYANG, infeliz", PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
							
						}
						
					} else if(asig.getValor() instanceof LlamadoFuncion) {
						
						if(checkFunctionType(asig, "YINYANG")) {
							
							error("Tu funcion no es de tipo YINYANG, infeliz",PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
							
						}
						
					} else {
						
						error("Pon bien el YINYANG, infeliz", PolePackage.Literals.ASIGNACION_VARIABLE__VALOR);
						
					}
					
				}
				
			}
			
		}
		
		
	}
	
	private void checkMethodAndFunctionParametersWithCall(LlamadoMetodo llamadoMetodo, LlamadoFuncion llamadoFuncion, String choice) {
		
		if(choice.equals("FUNCTION")) {
			
			LlamadoFuncion llamado = llamadoFuncion;
			int i = 0;
			
			for(Expresion expr : llamado.getArgs()) {
				
				if(!(getType(expr).equals(llamado.getFunc().getParam().get(i).getTipoDeDato()))) {
					
					if(getType(expr).equals("LLAMADOVARIABLE")) {
						
						if(checkVariableForOperatorType(expr, llamado.getFunc().getParam().get(i).getTipoDeDato())) {
							
							error("Tus argumentos no coinciden con los parametros de la funcion, imbecil", PolePackage.Literals.LLAMADO_FUNCION__ARGS);
							
							
						}
						
					} else if(getType(expr).equals("LLAMADOFUNCION")) {
						
						if(checkFunctionForOperatorType(expr, llamado.getFunc().getParam().get(i).getTipoDeDato())) {
							
							error("Tus argumentos no coinciden con los parametros de la funcion, imbecil", PolePackage.Literals.LLAMADO_FUNCION__ARGS);
							
						}
						
					} else if(!(getType(expr).equals("MRBEAST") && llamado.getFunc().getParam().get(i).getTipoDeDato().equals("XOKAS"))) {
						
						error("Tus argumentos no coinciden con los parametros de la funcion, imbecil", PolePackage.Literals.LLAMADO_FUNCION__ARGS);
						
					}
					
				}
				
				i++;
				
				
			}
			
		} else if(choice.equals("METHOD")) {
			
			LlamadoMetodo llamado = llamadoMetodo;
			int i = 0;
			
			for(Expresion expr : llamado.getArgs()) {
				
				if(!(getType(expr).equals(llamado.getMethod().getParam().get(i).getTipoDeDato()))) {
					
					if(getType(expr).equals("LLAMADOVARIABLE")) {
						
						if(checkVariableForOperatorType(expr, llamado.getMethod().getParam().get(i).getTipoDeDato())) {
							
							error("Tus argumentos no coinciden con los parametros de la funcion, imbecil", PolePackage.Literals.LLAMADO_METODO__ARGS);
							
							
						}
						
					} else if(getType(expr).equals("LLAMADOFUNCION")) {
						
						if(checkFunctionForOperatorType(expr, llamado.getMethod().getParam().get(i).getTipoDeDato())) {
							
							error("Tus argumentos no coinciden con los parametros de la funcion, imbecil", PolePackage.Literals.LLAMADO_METODO__ARGS);
							
						}
						
					} else if(!(getType(expr).equals("MRBEAST") && llamado.getMethod().getParam().get(i).getTipoDeDato().equals("XOKAS"))) {
						
						error("Tus argumentos no coinciden con los parametros de la funcion, imbecil", PolePackage.Literals.LLAMADO_METODO__ARGS);
						
					}
					
				}
				
				i++;
				
				
			}
			
		}
		
		
		
		
		
	}
		


		
	
	
	
	private boolean checkVariableType(Instruccion instr, String tipo) {
		
		if(instr instanceof DeclVariables) {
			
			DeclVariables decl = ((DeclVariables) instr);
			return (!((LlamadoVariable) decl.getValor()).getVariable().getTipoDeDato().equals(tipo));
			
		} else if(instr instanceof AsignacionVariable) {
			
			AsignacionVariable asig = ((AsignacionVariable) instr);
			return (!((LlamadoVariable) asig.getValor()).getVariable().getTipoDeDato().equals(tipo));
			
		}
		
		return false;
		
		
		
		
		
	}
	
	private boolean checkFunctionType(Instruccion instr, String tipo) {
		
		if(instr instanceof DeclVariables) {
			
			DeclVariables decl = ((DeclVariables) instr);
			return (!(((LlamadoFuncion) decl.getValor()).getFunc().getTipoDeDato().equals(tipo)));
			
		} else if (instr instanceof AsignacionVariable) {
			
			AsignacionVariable asig = ((AsignacionVariable) instr);
			return(!(((LlamadoFuncion) asig.getValor()).getFunc().getTipoDeDato().equals(tipo)));
			
		}

		
		return false;
		
		
		
	}
	
	private String getVariableType(Expresion expr) {
		
		return ((LlamadoVariable) expr).getVariable().getTipoDeDato();
		
	}
	
	private String getFunctionType(Expresion expr) {
		
		return ((LlamadoFuncion) expr).getFunc().getTipoDeDato();
		
	}
	
	private boolean checkVariableForOperatorType(Expresion expr, String tipo) {
		
		return (!(((LlamadoVariable) expr).getVariable().getTipoDeDato().equals(tipo)));
		
	}
	
	private boolean checkFunctionForOperatorType(Expresion expr, String tipo) {
		
		
		return (!(((LlamadoFuncion) expr).getFunc().getTipoDeDato().equals(tipo)));
		
	}
	
	private boolean compareTypesVariableCall(Expresion expr1, Expresion expr2) {
		
		return (((LlamadoVariable) expr1).getVariable().getTipoDeDato().equals(((LlamadoVariable) expr2).getVariable().getTipoDeDato()));
		
	}
	
	private boolean compareTypesFunctionCall(Expresion expr1, Expresion expr2) {
		
		return ((LlamadoFuncion) expr1).getFunc().getTipoDeDato().equals(((LlamadoFuncion) expr2).getFunc().getTipoDeDato());
		
	}
	
	private boolean compareTypesExprOneVariableCall(Expresion expr1, Expresion expr2) {
		
		String tipoDeDatoExpr = ((LlamadoVariable)expr1).getVariable().getTipoDeDato();
		return (tipoDeDatoExpr.equals("DALAS") && expr2 instanceof ExpresionString) ||(tipoDeDatoExpr.equals("YINYANG") && expr2 instanceof ExpresionBooleana) || 
				((tipoDeDatoExpr.equals("MRBEAST") || tipoDeDatoExpr.equals("XOKAS")) && expr2 instanceof ExpresionDecimal);
		
	}
	
	private boolean compareTypesExprOneFunctionCall(Expresion expr1, Expresion expr2) {
		
		String tipoDeDatoExpr = ((LlamadoFuncion)expr1).getFunc().getTipoDeDato();
		return (tipoDeDatoExpr.equals("DALAS") && expr2 instanceof ExpresionString) ||(tipoDeDatoExpr.equals("YINYANG") && expr2 instanceof ExpresionBooleana) || 
				((tipoDeDatoExpr.equals("MRBEAST") || tipoDeDatoExpr.equals("XOKAS")) && expr2 instanceof ExpresionDecimal);
		
	}
	
	private boolean compareFunctionAndVariableCalls(Expresion varCall, Expresion funcCall) {
		
		String tipoDatoVarCall = ((LlamadoVariable) varCall).getVariable().getTipoDeDato();
		String exprFuncCall = ((LlamadoFuncion) funcCall).getFunc().getTipoDeDato();
		return tipoDatoVarCall.equals(exprFuncCall);
		
		
		
		
		
	}
	
	private String getType(Expresion expr) { //
		
		if(expr instanceof ExpresionDecimal) {
		
			return "MRBEAST";
			
		}
		
		else if (expr instanceof ExpresionString) {
			
		
			return "DALAS";
				
					
		} else if(expr instanceof ExpresionBooleana) {
			
			
			return "YINYANG";
		
		}
		else if (expr instanceof LlamadoVariable) {
			
			return "LLAMADOVARIABLE";
			
		} else if(expr instanceof LlamadoFuncion) {
			
			return "LLAMADOFUNCION";
			
		} else {
			
			return "";
			
		}
		
	}
	
	private String getOperationType(Expresion expr) {
		
		if(expr instanceof ExpresionAritmetica) {
			
			return "OPAR";
			
		} else if (expr instanceof ExpresionConcatenacion) {
			
			return "OPC";
			
		} else if (expr instanceof OperacionesBooleanasDeComparacion) {
			
			return "OPBC";
			
		} else if(expr instanceof OperacionesBooleanasDeComparacionAritmetica) {
			
			return "OPBCA";
			
		} else if (expr instanceof OperadoresLogicosBooleanos) {
			
			return "OLB";
			
		} else if (expr instanceof OpNEG) {
			
			return "OPN";
			
		}
		
		return "";
		
	}
	
	
	
	
}
