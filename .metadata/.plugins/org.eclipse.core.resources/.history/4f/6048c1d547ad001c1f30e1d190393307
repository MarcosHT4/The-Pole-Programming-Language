								}
								
								
							} else {
								
								error("Pon bien el MRBEAST o el XOKAS infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
								
							}
							
						}
						
					}
				
				//PEREMNNAYA MRBEAST dedde -> SUMA JULIOPROFE "HOLA" Y f()
					//Crear getType
					
				} else if(getType(exprIzq).equals("LLAMADOFUNCION") || getType(exprDer).equals("LLAMADOFUNCION")) {
					
					if(getType(exprIzq).equals("LLAMADOFUNCION")) {
						
						if(checkFunctionForOperatorType(exprIzq, "MRBEAST") && checkFunctionForOperatorType(exprIzq, "XOKAS")) {
							
							error("Tu funcion de la izquierda no es MRBEAST o XOKAS, infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
							
						}
						
						if(!(getType(exprDer).equals("MRBEAST"))) {
							
							if(getType(exprDer).equals("LLAMADOVARIABLE")) {
								
								if((checkVariableForOperatorType(exprDer, "MRBEAST") && checkVariableForOperatorType(exprDer, "XOKAS"))) {
									
									error("Tu operacion no es completamente MRBEAST O XOKAS, infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
									
									
								}
								
							} else if(getType(exprDer).equals("LLAMADOFUNCION")) {
								
								if((checkFunctionForOperatorType(exprDer, "MRBEAST") && checkFunctionForOperatorType(exprDer, "XOKAS"))) {
									
									error("Tu operacion no es completamente MRBEAST o XOKAS, infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
									
								}
								
							} else {
								
								error("Pon bien el Dalas, infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
							}
							
						}
						
						
						
						
				} else if (getType(exprDer).equals("LLAMADOFUNCION")) {
					
						if(checkFunctionForOperatorType(exprDer, "MRBEAST") && checkFunctionForOperatorType(exprDer, "XOKAS")) {
							
							error("Tu funcion de la derecha no es MRBEAST o XOKAS, infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
							
						}
	
				}
				
			} else {
				
				error("No pusiste bien el MRBEAST o XOKAS, infeliz", PolePackage.Literals.EXPRESION_ARITMETICA__OP);
				
			}
		}
			
		} else if(expr instanceof ExpresionConcatenacion) {
			
			Expresion exprPrimero = ((ExpresionConcatenacion) expr).getPrimero();
			Expresion exprSegundo = ((ExpresionConcatenacion) expr).getSegundo();
			
			if(!(getType(exprPrimero).equals("DALAS") && getType(exprSegundo).equals("DALAS"))) {
				
				if(getType(exprPrimero).equals("LLAMADOVARIABLE") || getType(exprSegundo).equals("LLAMADOVARIABLE")) {
					
					if(getType(exprPrimero).equals("LLAMADOVARIABLE")) {
						
						if(checkVariableForOperatorType(exprPrimero, "DALAS")) {
							
							error("Tu expresion de la izquierda no es de tipo DALAS, infeliz", PolePackage.Literals.EXPRESION_CONCATENACION__PRIMERO);
							
						}
						
						if(!(getType(exprSegundo).equals("DALAS"))) {
							
							if(getType(exprSegundo).equals("LLAMADOVARIABLE")) {
								
								if((checkVariableForOperatorType(exprSegundo, "DALAS"))) {
									
									error("Tu operacion no es completamente DALAS, infeliz", PolePackage.Literals.EXPRESION_CONCATENACION__SEGUNDO);
									
									
								}
								
							} else if(getType(exprSegundo).equals("LLAMADOFUNCION")) {
								
								if((checkFunctionForOperatorType(exprSegundo, "DALAS"))) {
									
									error("Tu operacion no es completamente DALAS, infeliz", PolePackage.Literals.EXPRESION_CONCATENACION__SEGUNDO);
									
								}
								
							} else {
								
								error("Pon bien el DALAS, infeliz", PolePackage.Literals.EXPRESION_CONCATENACION__SEGUNDO);
							}
							
						}
						
					
							
					} else if(getType(exprSegundo).equals("LLAMADOVARIABLE")) {
						
						if(checkVariableForOperatorType(exprSegundo, "DALAS")) {
							
							error("Tu expresion de la derecha no es de tipo DALAS, infeliz", PolePackage.Literals.EXPRESION_CONCATENACION__SEGUNDO);
							
						}
						
						if(!(getType(exprPrimero).equals("DALAS"))) {
							
							if(getType(exprPrimero).equals("LLAMADOFUNCION")) {
								
								if(checkFunctionForOperatorType(exprPrimero, "DALAS")) {
									
