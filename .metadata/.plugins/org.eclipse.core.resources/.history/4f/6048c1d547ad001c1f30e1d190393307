					}
					
					if(!(getType(exprDer).equals("YINYANG"))) {
						
						if(getType(exprDer).equals("LLAMADOVARIABLE")) {
							
							if((checkVariableForOperatorType(exprDer, "YINYANG"))) {
								
								error("Tu operacion no es completamente YINYANG, infeliz", PolePackage.Literals.OPERADORES_LOGICOS_BOOLEANOS__DER);
								
								
							}
							
