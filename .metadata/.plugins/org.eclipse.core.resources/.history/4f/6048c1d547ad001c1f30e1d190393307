			}
			
		
	} else if(expr instanceof OperadoresLogicosBooleanos) {
		
		Expresion exprIzq = ((OperadoresLogicosBooleanos) expr).getIzq();
		Expresion exprDer = ((OperadoresLogicosBooleanos) expr).getDer();
		
		if(!(getType(exprIzq).equals("YINYANG") && getType(exprDer).equals("YINYANG"))) {
			
