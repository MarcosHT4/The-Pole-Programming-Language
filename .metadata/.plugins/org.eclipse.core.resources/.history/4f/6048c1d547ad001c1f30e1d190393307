		Expresion exprIzq = ((OperacionesBooleanasDeComparacion) expr).getIzq();
		Expresion exprDer = ((OperacionesBooleanasDeComparacion) expr).getDer();
		
		if(!(((getType(exprIzq).equals("MRBEAST") || getType(exprIzq).equals("XOKAS")) && ((getType(exprDer).equals("MRBEAST") || getType(exprDer).equals("XOKAS")))) ||
			(getType(exprIzq).equals("DALAS") && getType(exprDer).equals("DALAS")) ||
			(getType(exprIzq).equals("YINYANG") && getType(exprDer).equals("YINYANG")))) {
			
			if(getType(exprIzq).equals("LLAMADOVARIABLE") || getType(exprDer).equals("LLAMADOVARIABLE")) {
				
				if(getType(exprIzq).equals("LLAMADOVARIABLE") && getType(exprDer).equals("LLAMADOVARIABLE")) {
					
					if(!compareTypesVariableCall(exprIzq, exprDer)) {
						
						error("Tus expresiones no concuerdan, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
						
					}
					
				} else if(getType(exprIzq).equals("LLAMADOVARIABLE") && !(getType(exprDer).equals("LLAMADOVARIABLE"))) {
					
					if(getType(exprDer).equals("LLAMADOFUNCION")) {
						
						if(!compareFunctionAndVariableCalls(exprIzq, exprDer)) {
							
							error("Tu variable y tu funcion no concuerdan, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
							
						}
						
					} else {
					
					
						if(!compareTypesExprOneVariableCall(exprIzq, exprDer)) {
						
							error("Tu variable no concuerda, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
						
					}
					
					}
					
				} else if(!(getType(exprIzq).equals("LLAMADOVARIABLE")) && (getType(exprDer).equals("LLAMADOVARIABLE"))) {
					
					if(getType(exprIzq).equals("LLAMADOFUNCION")) {
						
						if(!compareFunctionAndVariableCalls(exprDer, exprIzq)) {
							
							error("Tu variable y tu funcion no concuerdam, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
							
						}
						
					} else {
						
						if(!compareTypesExprOneVariableCall(exprDer, exprIzq)) {
							
							error("Tu variable no concuerda, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
							
						}
						
					}
					
				}
				
			} else if(getType(exprIzq).equals("LLAMADOFUNCION") || getType(exprDer).equals("LLAMADOFUNCION")) {
				
				if(getType(exprIzq).equals("LLAMADOFUNCION") && getType(exprDer).equals("LLAMADOFUNCION")) {
					
					if(!compareTypesFunctionCall(exprIzq, exprDer)) {
						
						error("Tus expresiones no concuerdan, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
						
					} 
				} else if(getType(exprIzq).equals("LLAMADOFUNCION") && !(getType(exprDer).equals("LLAMADOFUNCION"))) {
						
						if(getType(exprDer).equals("LLAMADOVARIABLE")) {
							
							if(!compareFunctionAndVariableCalls(exprDer, exprIzq)) {
								
								error("Tu variable y tu funcion no concuerdan, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
								
							}
							
						} else {
						
						
							if(!compareTypesExprOneFunctionCall(exprIzq, exprDer)) {
							
								error("Tu variable no concuerda, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
							
						}
						
						}
						
					} else if(!(getType(exprIzq).equals("LLAMADOFUNCION")) && (getType(exprDer).equals("LLAMADOFUNCION"))) {
						
						if(getType(exprIzq).equals("LLAMADOVARIABLE")) {
							
							if(!compareFunctionAndVariableCalls(exprIzq, exprDer)) {
								
								error("Tu variable y tu funcion no concuerdan, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
								
							}
					
						} else {
					
					if(!compareTypesExprOneFunctionCall(exprDer, exprIzq)) {
						
						error("Tu variable no concuerda, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
						
					}
				
				}
				
			}
			
			else {
				
				error("Tus expresiones no concuerdan, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
				
			}
		
			
		
				
	} else {
		
		error("Tus expresiones, no concuerdan, infeliz", PolePackage.Literals.OPERACIONES_BOOLEANAS_DE_COMPARACION__IZQ);
		
	}
			
			
	
		}
		
	} else if(expr instanceof OpNEG) {
		
		Expresion exprNeg = ((OpNEG) expr).getExpr();
		
		if(!getType(exprNeg).equals("YINYANG")) {
			
			if(getType(exprNeg).equals("LLAMADOVARIABLE")) {
				
				if(checkVariableForOperatorType(exprNeg, "YINYANG")) {
					
					error("No puedes negar algo que no sea YINYANG, imbecil", PolePackage.Literals.OP_NEG__EXPR);
					
				}
				
			} else if (getType(exprNeg).equals("LLAMADOFUNCION")) {
				
				if(checkFunctionForOperatorType(exprNeg, "YINYANG")) {
					
					error("No puedes negar algo que no sea YINYANG, imbecil", PolePackage.Literals.OP_NEG__EXPR);
					
				}
				
			} else {
				
				error("No puedes negar algo que no sea YINYANG, imbecil", PolePackage.Literals.OP_NEG__EXPR);
				
			}
			
		} 
		
	} else if (expr instanceof OperacionesBooleanasDeComparacionAritmetica) {
		
		Expresion exprIzq = ((OperacionesBooleanasDeComparacionAritmetica) expr).getOp().getIzq();
